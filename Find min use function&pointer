#include<stdio.h>

int findmin(int*p, int n)
{
    int min=*p;

    int i;

    for(i=1 ; i<n ;i++) 
{
        if(*(p+i)<min) 
{
            min = *(p+i) ;
        }
    }

    return min;
}
int main()
{
    int arr[]={10, 20,30,5,6,8,90};

    int min = findmin(arr, 5);

    printf ("min=%d",min);

    return 0;
}
