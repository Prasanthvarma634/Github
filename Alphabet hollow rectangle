#include <stdio.h>
int main() {
    int i, j, n = 5 , m = 5;
    char ch = 'A';
    for (i = 1; i <= n; i++) {
        for (j = 1; j <= m; j++) {
            if (i == 1 || i == n || j == 1 || j == m) { printf("%c ", ch++);
           } else { 
               printf("  ");
        }
        }
        printf("\n");
    }
    
    return 0;
}
