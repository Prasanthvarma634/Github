#include <stdio.h>

int main() {

    int arr[]={10, 5,4,30,40,90} ;

    int *p = arr ;

    int min, smax ;

    int i;

    min = smax = *p ;

    for(i=1 ;i<6;i++)
{
        if(*(p+i)<min) 
{
            min = *(p+i) ;

        }

        if(*(p+i)>smax) 
{
            smax = *(p+i) ;
        }

    }
    printf ("min = %d\n" ,min) ;

    printf ("smax = %d" ,smax);

    return 0;

}
