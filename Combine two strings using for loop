#include <stdio.h>
#include <string.h>

int main() {
    char c[100];
    char n[50];
    int i;
printf("enter  th string1:");
fgets(c,sizeof(c),stdin);
printf("enter the string2:");
fgets (n, sizeof(n),stdin);
for(i=0 ; c[i]!='\0' ; i++){
    if(c[i]=='\n'){
    c[i]='\0';{
        break;
    }
}
}
for(i=0 ; n[i]!='\0' ; i++){
    if(n[i]=='\n'){
    n[i]='\0';{
        
        break;
    }
    }
        
    }
        
printf("%s%s",c,n);
    return 0;
}
